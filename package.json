  "scripts": {
    "watch:server": "tsc --watch --project tsconfig.node.json --noEmit",
    "dev:server": "nodemon",
    "dev:client": "vite",
    "build:server": "cross-env NODE_ENV=production rimraf dist && tsc --project tsconfig.node.json",
    "build:client": "tsc --project tsconfig.vite.json --noEmit && vue-tsc --noEmit && vite build -c vite.config.ts",
    "build": "npm run build:server && npm run build:client",
    "start:server": "cross-env NODE_ENV=production node -r dotenv/config dist/index.js",
    "start:client": "vite preview"
  }
